pipeline {
    agent any 
    options {
        timeout time: 5, unit: SECONDS
    }
    parameters {
        booleanParam name: 'sleep_time', defaultValue: 10, description: 'sleep time'
    }
    stages {
        stage("One") {
            steps {
                sleep 10
            }
            echo 'hello'
        }
    }
}
